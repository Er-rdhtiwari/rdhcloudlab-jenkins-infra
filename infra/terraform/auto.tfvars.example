project_name     = "jenkins-ec2"
env              = "dev"
instance_type    = "t3.medium"
root_volume_size = 30
key_name         = "jenkins-key"
# public_key_path = "~/.ssh/jenkins.pub"
allowed_ssh_cidr = "0.0.0.0/0"
root_domain      = "example.com"
jenkins_subdomain = "jenkins"
hosted_zone_id   = "Z1234567890"
auto_shutdown_hours = 6
enable_ssm_aws_creds = false
ssm_aws_creds_param_name = "/jenkins/aws-creds"
# jenkins_admin_email = "admin@example.com"
